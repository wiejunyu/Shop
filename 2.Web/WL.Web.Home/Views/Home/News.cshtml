
@{
    ViewBag.Title = "行业资讯_越南专线|越南跨境购|越南工业品采购|跨境物流链";
    ViewBag.Description = "【跨境物流链】专注越南专线,泰国专线,老挝专线柬埔寨专线,工业品采购,五金采购,跨境国际陆运,越南工业品采购越南跨境购,路运,送货到门,包清关,货到付款等增值服务";
    ViewBag.Keywords = "越南专线-泰国专线-老挝专线-柬埔寨专线-工业品采购-五金采购-跨境国际陆运-越南工业品采购-越南跨境购";
    Layout = "~/Views/Shared/MainMaster.cshtml";
}
@using WL.Home.Models
@using WL.Infrastructure.Common
<section>
    <div class="goods_item_box">
        <div class="wrapper clearfix">
            <div class="goods_item_list_article">
                @foreach (ArticleModels temp in ViewData["list"] as List<ArticleModels>)
                {
                    <figure onclick="javascript:window.location.href='@Html.Raw("/article/" + temp.id + ".html")';">
                        <div class=" goods_item_list_info">
                            <ul>
                                <li><img src="@Html.Raw(" http://admin.chengyunvip.com")@temp.thumb" title="@temp.title"></li>
                            </ul>
                            <ul>
                                <li><h3>@temp.title</h3></li>
                                <li><time><i class="fa fa-clock-o fa-fw"></i>@Common.ConvertDate(temp.createtime.ToString())</time>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;来源：跨境物流链</li>
                                <li>
                                    <p>
                                        @temp.description
                                        ...<br>
                                    </p>
                                </li>
                                <li>
                                    <a href="@Html.Raw("/article/" + temp.id + ".html")" title="@temp.title" class="more_detail">
                                        更多
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </figure>
                }
            </div>
        </div>
    </div>
</section>

