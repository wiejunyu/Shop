<script>
    var id = 0;
    function loginChangeCheckCode() {
        id++;
        $("#loginImg").attr("src", "/Data/ShowValidateCode?id=" + id);  //id=2
    }
    $(function () {
        $("#loginImg").attr("src", "/Data/ShowValidateCode?id=" + id);
        $("#login").submit(function () {
            _show();
            var url = $(this).attr('action');
            var data = $(this).serialize();

            var data_arr = $(this).serializeArray();

            if (data_arr[0].value == "") {
                _hide();
                message(true, "账号不能为空", "");
                return false;
            }
            if (data_arr[1].value == "") {
                _hide();
                message(true, "密码不能为空", "");
                return false;
            }
            if (data_arr[2].value == "") {
                _hide();
                message(true, "验证码不能为空", "");
                return false;
            }
            var jsondata = undefined;
            jsondata = $("#login").serializeObject();
            jsondata = { json: JSON.stringify(jsondata) };
            $.ajax({
                type: "POST",
                async: true,
                dataType: 'json',
                url: url,
                data: jsondata,
                success: function (json) {
                    if (json.message == '0') {
                        _hide();
                        loginChangeCheckCode();
                        message(json.error, "验证码错误", json.link);   //消息提示
                    }
                    else {
                        _hide();
                        message(json.error, json.message, json.link);   //消息提示
                    }
                }
            });
            return false;
        });
    });
    function LogOut() {
        _show();
        $.ajax({
            type: "POST",
            url: "/Data/LogOut",
            data: { },
            dataType: "json",
            success: function (data) {
                if (data == 1)
                {
                    message(false, "退出成功", "/index.html");
                }
                _hide();
            }
        });
    };
</script>